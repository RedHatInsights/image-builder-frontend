{
  "env": {
    "jest": true,
    "es6": true
  },
  "plugins": [
    "testing-library",
    "jest-dom"
  ],
  "rules": {
    "react/display-name": "off",
    "react/prop-types": "off",
    "testing-library/await-async-events": "off"
  },
  "extends": [
    "plugin:testing-library/react",
    "plugin:jest-dom/recommended"
  ]
}
